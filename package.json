{
  "name": "egg-sequelize-wrapper",
  "version": "0.0.7",
  "description": "封装了sequelize的增删改方法，where条件书写更加流畅，并可以选择对查询后结果进行预处理",
  "eggPlugin": {
    "name": "sequelizeWrapper",
    "dependencies": [
      "sequelize"
    ]
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin"
  ],
  "dependencies": {
    "sequelize": "^4.4.2",
    "inflection": "^1.12.0",
    "kind-of": "^6.0.2",
    "lodash": "^4.17.5",
    "moment": "^2.20.1"
  },
  "devDependencies": {
    "autod": "^3.0.0",
    "autod-egg": "^1.0.0",
    "egg": "^2.0.0",
    "egg-bin": "^4.3.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.13.0",
    "eslint": "^4.11.0",
    "eslint-config-egg": "^5.1.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.4"
  },
  "scripts": {
    "test": "npm run lint -- --fix && egg-bin pkgfiles && npm run test-local",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "egg-bin pkgfiles --check && npm run lint && npm run cov",
    "pkgfiles": "egg-bin pkgfiles",
    "autod": "autod"
  },
  "files": [
    "app.js",
    "agent.js",
    "config",
    "lib"
  ],
  "ci": {
    "version": "8, 9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dadiaoguai/egg-sequelize-wrapper.git"
  },
  "bugs": {
    "url": "https://github.com/dadiaoguai/egg-sequelize-wrapper/issues"
  },
  "homepage": "https://github.com/dadiaoguai/egg-sequelize-wrapper#readme",
  "author": "charlesliu",
  "license": "MIT"
}
